{ 
  "name": "Chess.com Chrome Extension", 
  "version": "1.2", 
  "description": "Gives information about your opponents on chess.com", 
  "background": { "scripts": ["background.js"] }, 
  "page_action" : 
  { 
    "default_icon" : "icon-19.png", 
    "default_title" : "Rapportive for Chess.com!"
  }, 
  "content_scripts" : [ 
    { 
      "matches" : ["http://www.live.chess.com/*"], 
      "matches" : ["http://www.chess.com/*"],
      "js" : ["jquery-2.0.3.min.js", "background.js", "content_script.js"], 
      "css" : ["insertedcss.css"], 
      "run_at" : "document_idle", 
      "all_frames" : false
    } 
  ], 
"web_accessible_resources" : ["jquery-2.0.3.min.js"],
  "permissions" : [ 
    "tabs", 
    "activeTab", 
    "contentSettings", 
    "http://*/*",
    "https://*/*"
], 
  "icons" : { 
    "48" : "icon-48.png", 
    "128" : "icon-128.png"
  }, 
  "manifest_version": 2 
}